<div class = 'col-md-11'>
	<%= f.label :first_name %>
</div>
<div class = 'col-md-11'>
	<%= f.text_field :first_name, autofocus: true, placeholder: 'First Name', class: "form-control", onblur: 'validate_text_field(this.id)' %>
</div>
<div class = 'col-md-1' id = 'err_user_profile_first_name'>
</div>

<div class = 'col-md-11'>
	<%= f.label :last_name %>
</div>
<div class = 'col-md-11'>
	<%= f.text_field :last_name, placeholder: 'Last Name', class: "form-control", onblur: 'validate_text_field(this.id)' %>
</div>
<div class = 'col-md-1' id = 'err_user_profile_last_name'>
</div>

<div class = 'col-md-11'>
	<%= f.label :phone_number %>
</div>
<div class = 'col-md-11'>
	<%= f.text_field :phone_number, placeholder: 'Phone Number', class: "form-control", onblur: 'validate_phone_number(this.id)' %>
</div>
<div class = 'col-md-1' id = 'err_user_profile_phone_number'>
</div>

<div class = 'col-md-11'>
	<%= f.label :email %>
</div>
<div class = 'col-md-11'>
	<%= f.email_field :email, placeholder: 'email', class: "form-control", onblur: 'validate_email(this.id)' %>
</div>
<div class = 'col-md-1' id = 'err_user_profile_email'>
</div>	
